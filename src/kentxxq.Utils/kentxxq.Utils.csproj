    <PackageProjectUrl>https://github.com/kentxxq/kentxxq.Tools</PackageProjectUrl>
    <RepositoryUrl>https://github.com/kentxxq/kentxxq.Tools</RepositoryUrl>
    <Authors>kentxxq</Authors>
    <Company>kentxxq.com</Company>
    <Version>1.6.1</Version>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SharpPcap" Version="5.4.0" />
    <PackageReference Include="System.DirectoryServices.Protocols" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
</Project>